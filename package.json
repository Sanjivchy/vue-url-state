{
  "name": "vue-url-state",
  "version": "1.0.1",
  "description": "A Vue 3 composable for syncing reactive state with URL query parameters",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "vue",
    "vue3",
    "composable",
    "query",
    "url",
    "state",
    "router",
    "typescript"
  ],
  "author": "Sanjiv Chaudhary sanjivchaudhary415@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sanjivchy/vue-url-state.git"
  },
  "homepage": "https://github.com/Sanjivchy/vue-url-state#readme",
  "bugs": {
    "url": "https://github.com/Sanjivchy/vue-url-state/issues"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.4",
    "@vitejs/plugin-vue": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "vue": "^3.4.0",
    "vue-router": "^4.3.0"
  }
}
