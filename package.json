{
  "name": "vue-url-state",
  "version": "0.2.0",
  "description": "A Vue 3 composable to sync reactive state with URL query parameters, supporting parsing, serialization, and debounce.",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepare": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "composable",
    "query",
    "url",
    "state",
    "router"
  ],
  "author": "Sanjiv Chaudhary <sanjivchaudhary415@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^6.7.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sanjivchy/vue-url-state.git"
  },
  "bugs": {
    "url": "https://github.com/Sanjivchy/vue-url-state/issues"
  },
  "homepage": "https://github.com/Sanjivchy/vue-url-state#readme",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    }
  }
}
